{
  "rabbit_version": "3.9.15",
  "rabbitmq_version": "3.9.15",
  "product_name": "RabbitMQ",
  "product_version": "3.9.15",
  "users": [
    {
      "name": "guest",
      "password_hash": "ftlu5CRbqi8FENPZhvXwfOYfJqNgJ0T5Q/63B95CnJlVGW2l",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": [
        "administrator"
      ],
      "limits": {}
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [
    {
      "value": {
        "ack-mode": "on-confirm",
        "prefetch-count": 10,
        "trust-user-id": false,
        "uri": "amqp://rmq0-us.rmq.local:5672"
      },
      "vhost": "/",
      "component": "federation-upstream",
      "name": "RMQ-US"
    }
  ],
  "global_parameters": [
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "RMQ-US-FEDERATION",
      "pattern": "^federated",
      "apply-to": "exchanges",
      "definition": {
        "federation-upstream": "RMQ-US"
      },
      "priority": 0
    }
  ],
  "queues": [
    {
      "name": "federated-ds",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "quorum"
      }
    }
  ],
  "exchanges": [
    {
      "name": "federated-direct",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "federated-direct",
      "vhost": "/",
      "destination": "federated-ds",
      "destination_type": "queue",
      "routing_key": "federated",
      "arguments": {}
    }
  ]
}
